<h2>
  <%= link_to 'Add New +', new_post_path, class: 'header-menu' %>
  My Posts
</h2>
<ul>
<% @posts.each do |post| %>
<li>
<%= link_to post.title, post_path(post.id) %>
<span class="command_class">
<%= link_to 'Edit', edit_post_path(post.id), class: 'command'%>
<%= link_to 'x', 
post_path(post.id), 
method: :delete, 
class: 'command_x',
data: { confirm: 'Sure?'}%>
</span>
</li>
<% end %>
</ul>